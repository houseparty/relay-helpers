{
    "env": {
        "build": {
            "presets": [
                "es2015",
                "stage-0",
                "react"
            ],
            "plugins": [
                "react-require",
                "transform-flow-strip-types",
                ["transform-builtin-extend", {
                  "globals": ["Promise"]
                }]
            ],
            "minified": false
        },
        "test": {
            "presets": [
                "es2015",
                "stage-0",
                "react"
            ],
            "plugins": [
                "react-require",
                "transform-flow-strip-types",
                "./testBabelRelayPlugin",
                ["transform-builtin-extend", {
                  "globals": ["Promise"]
                }]
            ],
            "minified": false
        }
    }
}
